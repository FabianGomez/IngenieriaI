!TransferTest methodsFor: 'testing' stamp: 'FG 5/29/2022 09:19:28' prior: 50474578!
test01TransferAffectsBalanceCorrectly

	Transfer register: anAmountToTransfer from: accountToWithdraw to: accountToDeposit .
	
	self assert: accountToDeposit balance equals: anAmountToTransfer.
	self assert: accountToWithdraw balance equals:  (- 1 * anAmountToTransfer). ! !
!TransferTest methodsFor: 'testing' stamp: 'FG 5/29/2022 09:20:07' prior: 50474641!
test02TransferAccountTransactionsAreRegistered

	| transfer |
	transfer := Transfer register: anAmountToTransfer from: accountToWithdraw to: accountToDeposit.
	
	self assert: (accountToWithdraw hasRegistered: transfer withdrawTransfer).
	self assert: (accountToDeposit hasRegistered: transfer depositTransfer).
! !
!TransferTest methodsFor: 'testing' stamp: 'FG 5/29/2022 09:20:33' prior: 50474760!
test03TransferAccountTransactionsKnowTheTransfer

	| transferRegistered  |

	transferRegistered := Transfer register: anAmountToTransfer from: accountToWithdraw to: accountToDeposit.
	
	
	self assert: transferRegistered withdrawTransfer transfer equals: transferRegistered.
	self assert: transferRegistered depositTransfer transfer equals: transferRegistered.
! !
!TransferTest methodsFor: 'testing' stamp: 'FG 5/29/2022 09:21:08' prior: 50475214!
test04TransferAccountTransactionsKnowValue

	| transferRegistered |
	
	transferRegistered := Transfer register: anAmountToTransfer from: accountToWithdraw to: accountToDeposit .
	
	
	self assert: transferRegistered withdrawTransfer value equals: anAmountToTransfer.
	self assert: transferRegistered depositTransfer value equals: anAmountToTransfer.
! !
