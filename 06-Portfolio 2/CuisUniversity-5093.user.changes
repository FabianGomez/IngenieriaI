test10PortfolioWithSeveralPortfoliosReturnsAccountSummaryOfItsPortfolioForAccountSummary

	|portfolio childPortfolio account anotherAccount accountSummary accountSummaryReport|
	
	portfolio := Portfolio new.
	childPortfolio := Portfolio new.
	account := ReceptiveAccount new.
	Deposit register: 50 on: account.
	Withdraw register: 10 on: account.
	anotherAccount := ReceptiveAccount new.
	Deposit register: 40 on: anotherAccount.
	
	portfolio add: account.
	childPortfolio add: anotherAccount.
	portfolio add: childPortfolio.
		
	accountSummaryReport := AccountSummaryReport new.
	accountSummary := accountSummaryReport accountSummary: portfolio.

	self assert: accountSummary size equals: 4.
	self assert: (accountSummary at: 1) equals: 'Depsito por 50.'.
	self assert: (accountSummary at: 2) equals: 'Extraccin por 10.'.
	self assert: (accountSummary at: 3) equals: 'Depsito por 40.'.
	self assert: (accountSummary at: 4) equals: 'Balance = 80'.
		
		! !
