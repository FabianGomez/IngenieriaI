	account := ReceptiveAccount new.
	Deposit register: 50 on: account.
	Withdraw register: 10 on: account.
	Deposit register: 40 on: account.
	
	portfolio add: account.
	
	accountSummaryReport := AccountSummaryReport for: account.
	linesForAccount := accountSummaryReport accountSummary.
	
	portfolioAccountSummaryReport := AccountSummaryReport for: portfolio.
	linesForPortfolio := portfolioAccountSummaryReport accountSummary.

	self assert: linesForAccount equals: linesForPortfolio.! !

