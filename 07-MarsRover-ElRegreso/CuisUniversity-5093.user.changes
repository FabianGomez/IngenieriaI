test01LoggerOverPositionChangesShouldBeEmptyAfterProcesingEmptyCommand

	| marsRover marsRoverProxy logger |
	
	marsRover := MarsRover at: 1@1 heading: MarsRoverHeadingNorth.
	marsRoverProxy := MarsRoverProxy of: marsRover.
